{
  "name": "takadao.takaturn.comtracts",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": ">=17",
    "npm": "please-use-yarn",
    "yarn": ">= 1.22"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.6.1",
    "crypto": "^1.0.1",
    "erc-20-abi": "^1.0.0",
    "exceljs": "^4.3.0",
    "hardhat-helpers": "^0.1.51",
    "hardhat-tracer": "^2.5.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.2",
    "@nomicfoundation/hardhat-ethers": "^3.0.5",
    "@nomicfoundation/hardhat-verify": "^2.0.2",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "@openzeppelin/contracts": "^4.9.2",
    "chai": "^4.3.7",
    "dotenv": "^16.3.1",
    "ethereum-waffle": "^4.0.10",
    "ethers": "^6.9.0",
    "hardhat": "^2.19.2",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-deploy": "^0.11.44",
    "hardhat-deploy-ethers": "^0.4.1",
    "hardhat-gas-reporter": "^1.0.9",
    "prettier": "^2.8.8",
    "prettier-plugin-solidity": "^1.1.3",
    "solhint": "^3.4.1",
    "solidity-coverage": "^0.8.3"
  },
  "scripts": {
    "compile": "yarn hardhat compile",
    "n": "yarn hardhat node",
    "slither": "slither . --solc-remaps '@openzeppelin=node_modules/@openzeppelin' --exclude naming-convention,low-level-calls --buidler-ignore-compile",
    "format": "yarn prettier --write .",
    "test": "yarn hardhat test",
    "test:staging:mainnet": "yarn hardhat test --network mainnet_arbitrum",
    "test:staging:takadao": "yarn hardhat test --network takadao_private --grep",
    "test:staging:goerli": "yarn hardhat test --network testnet_arbitrum_goerli",
    "test:staging:sepolia": "yarn hardhat test --network testnet_arbitrum_sepolia",
    "coverage": "yarn hardhat coverage",
    "contractsInfo": "yarn hardhat run scripts/contractsInfo.js --network",
    "eventSignatures": "yarn hardhat run scripts/eventSignatures.js",
    "eventUrls": "yarn hardhat run scripts/tx-urls.js",
    "deploy": "yarn hardhat deploy",
    "deploy:local": "yarn hardhat deploy --tags takaturn_deploy",
    "deploy:takadao:mocks": "yarn hardhat deploy --network takadao_private --tags takadao_mocks",
    "deploy:takadao": "yarn hardhat deploy --network takadao_private --tags takadao_main",
    "deploy:mainnet": "yarn hardhat deploy --network mainnet_arbitrum --tags takaturn_deploy",
    "deploy:goerli": "yarn hardhat deploy --network testnet_arbitrum_goerli --tags takaturn_deploy",
    "deploy:sepolia": "yarn hardhat deploy --network testnet_arbitrum_sepolia --tags takaturn_deploy",
    "deploy:mainnet:multisig": "yarn hardhat deploy --network mainnet_arbitrum --tags multisig",
    "deploy:goerli:multisig": "yarn hardhat deploy --network testnet_arbitrum_goerli --tags multisig",
    "upgrade:local": "yarn hardhat deploy --tags takaturn_upgrade",
    "upgrade:mainnet": "yarn hardhat deploy --network mainnet_arbitrum --tags takaturn_upgrade",
    "upgrade:goerli": "yarn hardhat deploy --network testnet_arbitrum_goerli --tags takaturn_upgrade",
    "upgrade:sepolia": "yarn hardhat deploy --network testnet_arbitrum_sepolia --tags takaturn_upgrade",
    "deploy:sepolia:multisig": "yarn hardhat deploy --network testnet_arbitrum_sepolia --tags multisig",
    "upgrade:mainnet:multisig": "yarn hardhat deploy --network mainnet_arbitrum --tags multisig",
    "create:term:goerli": "yarn hardhat run scripts/contractInteractions/createTerm.js --network testnet_arbitrum_goerli",
    "create:term:sepolia": "yarn hardhat run scripts/contractInteractions/createTerm.js --network testnet_arbitrum_sepolia",
    "join:term:goerli": "yarn hardhat joinTerm --network testnet_arbitrum_goerli --term-id",
    "join:term:sepolia": "yarn hardhat joinTerm --network testnet_arbitrum_sepolia --term-id",
    "pay:term:goerli": "yarn hardhat payContribution --network testnet_arbitrum_goerli --term-id",
    "pay:term:sepolia": "yarn hardhat payContribution --network testnet_arbitrum_sepolia --term-id",
    "autoPay:term:goerli": "yarn hardhat toggleAutoPay --network testnet_arbitrum_goerli --term-id",
    "autoPay:term:sepolia": "yarn hardhat toggleAutoPay --network testnet_arbitrum_sepolia --term-id",
    "withdraw:fund:term:goerli": "yarn hardhat withdrawFund --network testnet_arbitrum_goerli --term-id",
    "withdraw:fund:term:sepolia": "yarn hardhat withdrawFund --network testnet_arbitrum_sepolia --term-id",
    "withdraw:collateral:term:goerli": "yarn hardhat withdrawCollateral --network testnet_arbitrum_goerli --term-id",
    "withdraw:collateral:term:sepolia": "yarn hardhat withdrawCollateral --network testnet_arbitrum_sepolia --term-id",
    "start:term:mainnet": "yarn hardhat startTerm --network mainnet_arbitrum --term-id",
    "start:term:goerli": "yarn hardhat startTerm --network testnet_arbitrum_goerli --term-id",
    "start:term:sepolia": "yarn hardhat startTerm --network testnet_arbitrum_sepolia --term-id",
    "close:cycle:mainnet": "yarn hardhat closeFundingPeriod --network mainnet_arbitrum --term-id",
    "close:cycle:goerli": "yarn hardhat closeFundingPeriod --network testnet_arbitrum_goerli --term-id",
    "close:cycle:sepolia": "yarn hardhat closeFundingPeriod --network testnet_arbitrum_sepolia --term-id",
    "start:cycle:mainnet": "yarn hardhat startNewCycle --network mainnet_arbitrum --term-id",
    "start:cycle:goerli": "yarn hardhat startNewCycle --network testnet_arbitrum_goerli --term-id",
    "start:cycle:sepolia": "yarn hardhat startNewCycle --network testnet_arbitrum_sepolia --term-id",
    "summary:term:mainnet": "yarn hardhat termSummary --network mainnet_arbitrum --term-id",
    "summary:term:goerli": "yarn hardhat termSummary --network testnet_arbitrum_goerli --term-id",
    "summary:term:sepolia": "yarn hardhat termSummary --network testnet_arbitrum_sepolia --term-id",
    "summary:fund:mainnet": "yarn hardhat fundSummary --network mainnet_arbitrum --term-id",
    "summary:fund:goerli": "yarn hardhat fundSummary --network testnet_arbitrum_goerli --term-id",
    "summary:fund:sepolia": "yarn hardhat fundSummary --network testnet_arbitrum_sepolia --term-id",
    "summary:collateral:mainnet": "yarn hardhat collateralSummary --network mainnet_arbitrum --term-id",
    "summary:collateral:goerli": "yarn hardhat collateralSummary --network testnet_arbitrum_goerli --term-id",
    "summary:collateral:sepolia": "yarn hardhat collateralSummary --network testnet_arbitrum_sepolia --term-id",
    "summary:yield:mainnet": "yarn hardhat yieldSummary --network mainnet_arbitrum --term-id",
    "summary:user:mainnet": "yarn hardhat userSummary --network mainnet_arbitrum --user-address",
    "summary:user:goerli": "yarn hardhat userSummary --network testnet_arbitrum_goerli --user-address",
    "summary:user:sepolia": "yarn hardhat userSummary --network testnet_arbitrum_sepolia --user-address",
    "summary:user:id:mainnet": "yarn hardhat userSummaryByTermId --network mainnet_arbitrum --user-address",
    "summary:user:id:goerli": "yarn hardhat userSummaryByTermId --network testnet_arbitrum_goerli --user-address",
    "summary:user:id:sepolia": "yarn hardhat userSummaryByTermId --network testnet_arbitrum_sepolia --user-address",
    "usdcMint:sepolia": "yarn hardhat usdcMint --network testnet_arbitrum_sepolia --user-address",
    "test:multisig": "yarn hardhat deploy --network testnet_ethereum_sepolia --tags multisig"
  }
}
